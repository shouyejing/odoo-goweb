<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="index" inherit_id='point_of_sale.index' name="Restaurant Index">&lt;!DOCTYPE html&gt;
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/marcos_pos/static/src/css/marcos_pos.css"/>
            </xpath>
        </template>

        <template id="assets_frontend" inherit_id="web.assets_common">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/marcos_pos/static/src/js/pos.js"></script>
            </xpath>
        </template>


    </data>
</openerp>