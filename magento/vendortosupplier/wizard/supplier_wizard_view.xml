<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
	<record id="id_supplier_wizard_form" model="ir.ui.view">
		<field name="name">supplier.wizard</field>
		<field name="model">supplier.wizard</field>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<form string="Odoo" version="7.0">
				<separator string="Supplier Token Key" colspan="6"/>
				<group>
					<field name="supplier_token_key"/>
				</group>
				<newline/>
				<separator colspan="6"/>
				<footer>
                    <button name="buttun_validate" string="Validate" type="object" class="oe_highlight"/>
					<button special="cancel" class="oe_highlight" icon="gtk-cancel" string="Close" />
				</footer>
			</form>
		</field>
	</record>
</data>	
</openerp>
